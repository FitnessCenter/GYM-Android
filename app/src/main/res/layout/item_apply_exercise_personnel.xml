<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>
        <variable name="userItem"
            type="com.dsm.gym.presentation.model.UserInfoModel"/>
    </data>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/apply_exercised_student_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@{@string/apply_exercise_student_tv(userItem.studentNumber,userItem.studentName)}"
        android:textSize="14sp"
        android:textColor="@color/colorBlack"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
</layout>